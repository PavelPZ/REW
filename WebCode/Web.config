  http://go.microsoft.com/fwlink/?LinkId=301879
  -->
<configuration>
  <appSettings configSource="Web.AppSetting.config" />
  <connectionStrings configSource="Web.ConnectionStrings.config" />
  <system.web>
    <compilation debug="true" targetFramework="4.6" />
    <httpRuntime targetFramework="4.6" />
  </system.web>
  <system.webServer> 
    <rewrite>
      <rules>
        <rule name="Preserve" stopProcessing="true">
          <match url="code/newea.aspx" />
          <action type="None" />
        </rule>
        <rule name="Preserve2" stopProcessing="true">
          <match url="links.aspx" />
        </rule>
        <rule name="Preserve3" stopProcessing="true">
          <match url="service.ashx" />
          <action type="None" />
        </rule>
        <rule name="RedirectAll" stopProcessing="true">
          <match url="(.*)" />
          <action type="Redirect" url="http://localhost/web4/{R:1}" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
  <runtime>
  </runtime>
  <system.codedom>
  </system.codedom>
</configuration>
